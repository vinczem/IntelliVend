{
  "name": "IntelliVend",
  "version": "1.0.3",
  "slug": "intellivend",
  "description": "Intelligens italautomata rendszer",
  "url": "https://github.com/vinczem/IntelliVend",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Web UI"
  },
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:glass-cocktail",
  "panel_title": "IntelliVend",
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "auth_api": false,
  "host_network": false,
  "map": ["config:rw", "share:rw"],
  "services": ["mqtt:want"],
  "options": {
    "mqtt_broker": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "log_level": "info"
  },
  "schema": {
    "mqtt_broker": "str",
    "mqtt_port": "port",
    "mqtt_user": "str?",
    "mqtt_password": "password?",
    "log_level": "list(debug|info|warning|error)"
  }
}
